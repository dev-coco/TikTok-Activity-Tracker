{
  "manifest_version": 3,
  "name": "抖音记录",
  "version": "1.0.0",
  "description": "获取抖音关注数、粉丝数、好友数。",
  "author": "Raz1ner",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "icons": {
    "128": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "storage",
    "offscreen"
  ],
  "host_permissions": [
    "https://script.google.com/*",
    "*://*.tiktok.com/*"
  ]
}